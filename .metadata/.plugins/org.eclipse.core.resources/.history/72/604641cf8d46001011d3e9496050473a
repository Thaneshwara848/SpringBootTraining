package com.example.demo.modelorEntriy;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.Getter;
import lombok.Setter;

@Entity
@Getter
@Setter
public class Employee {

	@Id
	private int id;
	private String name;
	private int age;
	private int salary;
	private String desig;
	

	
	
}

					
						




